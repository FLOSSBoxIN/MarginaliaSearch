<h1>Services</h1>
<table id="services" class="table">
    <tr>
        <th>Service ID</th>
        <th>UUID</th>
        <th>Last Seen (ms)</th>
    </tr>
    {{#each services}}
    <tr class="{{#if isMissing}}missing{{/if}} {{#unless alive}}terminated{{/unless}}">
        <td><a href="/services/{{serviceId}}">{{serviceId}}</a></td>
        <td>{{{readableUUID uuidFull}}}</td>
        <td>{{lastSeenMillis}}</td>
    </tr>
    {{/each}}
</table>